{
  "homepage": "https://batstolya.github.io/product_catalog",
  "name": "react-typescript-starter-pack",
  "version": "0.1.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.10",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "classnames": "^2.3.2",
    "node-sass": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.8.1",
    "react-scripts": "4.0.3",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "typescript": "^4.2.4",
    "typescript-eslint-parser": "^22.0.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "format": "prettier --write . --ignore-path .gitignore",
    "lint": "eslint ./src",
    "fix-style": "npm run format && npm run lint -- --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "^0.0.4",
    "@types/react-slick": "^0.23.10",
    "eslint": "^7.25.0",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4"
  }
}
